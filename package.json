{
  "name": "@vtaits/react-filterlist",
  "version": "0.1.0",
  "description": "React wrapper above @vtaits/filterlist",
  "main": "lib/index.js",
  "repository": "git@github.com:vtaits/react-filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\"",
    "build": "yarn clean && yarn build:cjs && yarn build:es",
    "lint": "eslint src --ext .js,.jsx",
    "test:unit": "jest",
    "test": "yarn lint && yarn test:unit",
    "prepublish": "yarn build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^0.1.0",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@vtaits/filterlist": "^0.1.1",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fbemitter": "^2.1.1",
    "jest-cli": "^23.6.0",
    "raf": "^3.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "prop-types": "^15.6.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "setupFiles": [
      "./setup-jest.js"
    ]
  }
}
