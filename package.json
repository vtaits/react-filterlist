{
  "name": "@vtaits/react-filterlist",
  "version": "0.1.4",
  "description": "React wrapper above @vtaits/filterlist",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "/es",
    "/lib"
  ],
  "repository": "git@github.com:vtaits/react-filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "start": "cross-env BABEL_ENV=dev webpack-dev-server",
    "clean": "rm -rf lib es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\"",
    "build": "yarn clean && yarn build:cjs && yarn build:es",
    "lint": "eslint src --ext .js,.jsx",
    "test:unit": "jest",
    "test": "yarn lint && yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^0.1.0",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@vtaits/filterlist": "^0.1.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "fbemitter": "^2.1.1",
    "jest-cli": "^24.7.1",
    "path": "^0.12.7",
    "qs": "^6.7.0",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "is-promise": "^2.1.0",
    "prop-types": "^15.7.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "setupFiles": [
      "./setup-jest.js"
    ]
  }
}
